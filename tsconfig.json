{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/svg/*": [
        "public/svg/*"
      ],
      "@/img/*": [
        "public/img/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js"
  },
  "extends": "astro/tsconfigs/base"
}